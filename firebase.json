{
  "hosting": {
    "target": "main",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "region": "asia-southeast1"
  }
}